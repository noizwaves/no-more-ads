{
    "name": "No More Ads",
    "version": "0.1",
    "description": "Ad blocker for Chrome",
    "background": {
        "scripts": [
            "backgroundWebRequest.js"
        ],
        "persistent": true
    },
    "permissions": [
        "activeTab",
        "declarativeContent",
        "storage",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        }
    },
    "options_page": "options.html",
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "manifest_version": 2
}